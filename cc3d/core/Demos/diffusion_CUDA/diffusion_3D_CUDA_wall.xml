 <CompuCell3D>
    <Metadata>
    <VirtualProcessingUnits ThreadsPerVPU="1">4</VirtualProcessingUnits>
 </Metadata> 
 
<Potts>
   <Dimensions x="256" y="256" z="256"/>
   <Anneal>0</Anneal>
   <Steps>100</Steps>
   <Temperature>0</Temperature>
   <Flip2DimRatio>0.0</Flip2DimRatio>
   <Boundary_y>NoFlux</Boundary_y>
 </Potts>



<Plugin Name="CellType">
    <CellType TypeName="Medium" TypeId="0"/>
    <CellType TypeName="Wall1" TypeId="1"/>
    <CellType TypeName="Wall2" TypeId="2"/>

 </Plugin>


 <Steppable Type="FlexibleDiffusionSolverFE_CUDA">
<!--  <Steppable Type="FastDiffusionSolver2DFE"> -->

	<DiffusionField>
        <DiffusionData>
	  
            <FieldName>FGF</FieldName>
            <ExtraTimesPerMCS>0</ExtraTimesPerMCS>
            <DiffusionConstant>0.01</DiffusionConstant>
            <DecayConstant>0.000</DecayConstant> 
            
            <DiffusionCoefficient CellType="Medium">0.1</DiffusionCoefficient>
            <DiffusionCoefficient CellType="Wall1">0.0</DiffusionCoefficient>
            <DiffusionCoefficient CellType="Wall2">0.0</DiffusionCoefficient>
        
            
            <!-- <DecayCoefficient>0.000</DecayCoefficient> -->
            
	    <ConcentrationFileName>Demos/diffusion_CUDA/diffusion_3D_CUDA.pulse.txt</ConcentrationFileName>
        </DiffusionData>
        <SecretionData>
            <Secretion Type="Wall1">1.1</Secretion>
        </SecretionData>
    </DiffusionField>
 </Steppable>

  <Steppable Type="PIFInitializer">
     <PIFName>Demos/diffusion_CUDA/diffusion_wall_3D_CUDA.piff</PIFName>
  </Steppable> 

</CompuCell3D>
