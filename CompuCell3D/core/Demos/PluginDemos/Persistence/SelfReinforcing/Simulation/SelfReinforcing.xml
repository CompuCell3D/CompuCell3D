<CompuCell3D Revision="2" Version="4.6.0">
   
   <Metadata>
      <NumberOfProcessors>1</NumberOfProcessors>
      <DebugOutputFrequency>10</DebugOutputFrequency>
   </Metadata>
   
   <Potts>
      <Dimensions x="100" y="100" z="1"/>
      <Steps>100000</Steps>
      <Temperature>10.0</Temperature>
      <NeighborOrder>1</NeighborOrder>
      <Boundary_x>Periodic</Boundary_x>
      <Boundary_y>Periodic</Boundary_y>
   </Potts>
   
   <Plugin Name="CellType">
      <CellType TypeId="0" TypeName="Medium"/>
      <CellType TypeId="1" TypeName="Cell"/>
   </Plugin>
   
   <Plugin Name="Volume">
      <VolumeEnergyParameters CellType="Cell" LambdaVolume="2.0" TargetVolume="36"/>
   </Plugin>

   <Plugin Name="Surface">
      <SurfaceEnergyParameters CellType="Cell" LambdaSurface="2.0" TargetSurface="60"/>
      <NeighborOrder>2</NeighborOrder>
   </Plugin>

   <Plugin Name="CenterOfMass"/>
   
   <Plugin Name="Persistence">
      <!-- Module adding persistent cell motility models -->
      <!-- Self-reinforcing model -->
      <PersistenceModel CellType="Cell" Model="Self-Reinforcing" Period="50">
         <ForceMode>Extension</ForceMode>
         <WorkTerm>Regular</WorkTerm>
         <Magnitude>
            <Constant>20.0</Constant>
         </Magnitude>
         <VectorInit>
            <RotateVector Axis="Z" Units="Degrees">
               <Constant>180.0</Constant>
            </RotateVector>
         </VectorInit>
      </PersistenceModel>
   </Plugin>

</CompuCell3D>
