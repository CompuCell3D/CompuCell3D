<CompuCell3D Revision="20190517" Version="3.7.10">

    <Potts>

        <!-- Basic properties of CPM (GGH) algorithm -->
        <Dimensions x="100" y="100" z="1"/>
        <Steps>100000</Steps>
        <Temperature>10.0</Temperature>
        <NeighborOrder>1</NeighborOrder>
        <Boundary_x>Periodic</Boundary_x>
        <Boundary_y>Periodic</Boundary_y>
    </Potts>

    <Plugin Name="CellType">

        <!-- Listing all cell types in the simulation -->
        <CellType TypeId="0" TypeName="Medium"/>
        <CellType TypeId="1" TypeName="MotCell"/>
        <CellType TypeId="2" TypeName="MotCell2"/>
    </Plugin>

    <Plugin Name="Volume">
        <VolumeEnergyParameters CellType="MotCell" LambdaVolume="4.0" TargetVolume="36"/>
        <VolumeEnergyParameters CellType="MotCell2" LambdaVolume="4.0" TargetVolume="36"/>
    </Plugin>

    <Plugin Name="Contact">
        <!-- Specification of adhesion energies -->
        <Energy Type1="Medium" Type2="Medium">10.0</Energy>
        <Energy Type1="Medium" Type2="MotCell">10.0</Energy>
        <Energy Type1="Medium" Type2="MotCell2">10.0</Energy>
        <Energy Type1="MotCell" Type2="MotCell">10.0</Energy>
        <Energy Type1="MotCell" Type2="MotCell2">10.0</Energy>
        <Energy Type1="MotCell2" Type2="MotCell2">10.0</Energy>
        <NeighborOrder>1</NeighborOrder>
    </Plugin>

    <Plugin Name="ImplicitMotility">
        <MotilityParameters CellType="MotCell" LambdaMotility="20"/>
        <MotilityParameters CellType="MotCell2" LambdaMotility="15"/>
    </Plugin>


    <Steppable Type="BlobInitializer">

        <!-- Initial layout of cells in the form of spherical (circular in 2D) blob -->
        <Region>
            <Center x="50" y="50" z="0"/>
            <Radius>3</Radius>
            <Gap>0</Gap>
            <Width>6</Width>
            <Types>MotCell</Types>
        </Region>
    </Steppable>
</CompuCell3D>
