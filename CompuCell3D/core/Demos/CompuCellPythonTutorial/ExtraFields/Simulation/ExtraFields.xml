 <CompuCell3D>
<!--  <PythonScript>examples_PythonTutorial/ExtraFields/ExtraFields.py</PythonScript> -->
 <Potts>
   <Dimensions x="55" y="55" z="10"/>
   <Anneal>0</Anneal>
   <Steps>1000</Steps>
   <Temperature>15</Temperature>
   <Flip2DimRatio>0.0</Flip2DimRatio>
   <Boundary_y>NoFlux</Boundary_y>
 </Potts>



<Plugin Name="CellType">
    <CellType TypeName="Medium" TypeId="0"/>
    <CellType TypeName="Condensing" TypeId="1"  Freeze=""/>
    <CellType TypeName="NonCondensing" TypeId="2" Freeze=""/>
 </Plugin>

 <Steppable Type="BlobInitializer">
   <Gap>0</Gap>
   <Width>5</Width>
   <CellSortInit>yes</CellSortInit>   
   <Radius>20</Radius>
 </Steppable>

 <Steppable Type="DiffusionSolverFE">
     <DiffusionField>
        <DiffusionData>
            <FieldName>FGF</FieldName>
            <DiffusionConstant>0.010</DiffusionConstant>
            <DecayConstant>0.000</DecayConstant>	    
	    <!--DoNotDecayIn>Medium</DoNotDecayIn-->		
<!--            <DeltaT>0.1</DeltaT> -->
<!--            <DeltaX>1.0</DeltaX> -->
            <!--<DoNotDiffuseTo>Amoeba</DoNotDiffuseTo> -->
        </DiffusionData>
    </DiffusionField>
 </Steppable>


</CompuCell3D>
